#: com/patrikdufresne/minarca/ui/SettingsDialog.java:681
msgid "Can't change backup schedule!"
msgstr "Vous ne pouvez pas changer la fréquence de sauvegarde!"

#: com/patrikdufresne/minarca/ui/SettingsDialog.java:671
msgid "Fail to retrieve current backup schedule."
msgstr "Impossible de récupérer la fréquence de sauvegarde actuelle."

#: com/patrikdufresne/minarca/ui/SettingsDialog.java:682
